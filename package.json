{
  "name": "archive-batch-editor",
  "version": "2.0.0",
  "type": "module",
  "description": "Batch metadata editor for Archive.org items",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "server": "node --env-file=.env --import tsx server/index.ts",
    "server:dev": "nodemon --exec \"node --env-file=.env --import tsx server/index.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "google-auth-library": "^10.3.0",
    "googleapis": "^158.0.0",
    "helmet": "^7.1.0",
    "multer": "^2.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.10.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "jest": "^30.1.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Allen Hsieh",
  "license": "MIT"
}
